{
  "name": "@mapray/ui",
  "version": "0.9.3",
  "description": "UI Engine for mapray-js",
  "keywords": [
    "3D",
    "sdk",
    "api",
    "webgl",
    "map",
    "geospatial",
    "globe",
    "ui",
    "operation"
  ],
  "type": "module",
  "main": "./dist/umd/maprayui.js",
  "module": "./dist/es/maprayui.js",
  "types": "./dist/es/@types/index.d.ts",
  "sideEffects": true,
  "directories": {
    "doc": "doc"
  },
  "exports": {
    ".": {
      "types": "./dist/es/@types/index.d.ts",
      "import": "./dist/es/maprayui.js",
      "require": "./dist/umd/maprayui.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "rollup -c --environment BUILD:production",
    "build-min": "rollup -c --environment BUILD:production,MINIFY:true",
    "build-devel": "rollup -c --environment BUILD:dev",
    "build-watch": "rollup -c --environment BUILD:dev -w",
    "css": "postcss -o dist/mapray.css src/css/mapray.css",
    "css-dev": "postcss --watch -o dist/mapray.css src/css/mapray.css",
    "doc": "typedoc --tsconfig tsconfig.json"
  },
  "contributors": [
    {
      "name": "Yasuhisa Tanioka"
    },
    {
      "name": "Daisuke Matsumoto",
      "url": "https://github.com/daisuke201"
    },
    {
      "name": "masa-sony"
    },
    {
      "name": "Matsuzaki Shota",
      "url": "https://github.com/ShotaMatsuzaki"
    },
    {
      "name": "sc-onishi"
    },
    {
      "name": "SGA-sato-takamasa"
    },
    {
      "name": "m-watakabe"
    }
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/sony/mapray-js.git"
  },
  "bugs": {
    "url": "https://github.com/sony/mapray-js/issues"
  },
  "homepage": "https://mapray.com",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "@types/node": "^20.2.3",
    "babel-loader": "^9.1.2",
    "core-js": "^3.6.4",
    "cross-env": "^7.0.0",
    "cssnano": "^6.0.1",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "postcss-inline-svg": "^6.0.0",
    "postcss-svgo": "^6.0.0",
    "rollup": "^3.15.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "tslib": "^2.2.0"
  },
  "peerDependencies": {
    "@mapray/mapray-js": "0.9.3"
  }
}
